#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jose/AdaptMx/bump-version/node_modules/esprima/bin/node_modules:/Users/jose/AdaptMx/bump-version/node_modules/esprima/node_modules:/Users/jose/AdaptMx/bump-version/node_modules:/Users/jose/AdaptMx/node_modules:/Users/jose/node_modules:/Users/node_modules:/node_modules:/Users/jose/AdaptMx/bump-version/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jose/AdaptMx/bump-version/node_modules/esprima/bin/node_modules:/Users/jose/AdaptMx/bump-version/node_modules/esprima/node_modules:/Users/jose/AdaptMx/bump-version/node_modules:/Users/jose/AdaptMx/node_modules:/Users/jose/node_modules:/Users/node_modules:/node_modules:/Users/jose/AdaptMx/bump-version/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
